<?xml version="1.0" encoding="UTF-8"?>
<CredentialStructure xmlns="http://www.zurich.ibm.com/security/idemix"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" 
    xmlns:xs="http://www.w3.org/2001/XMLSchema"
    xsi:schemaLocation="http://www.zurich.ibm.com/security/idemix CredentialStructure.xsd">
    
    <Attributes>
        <Attribute issuanceMode="known" name="expiry" type="int" />
        <Attribute issuanceMode="known" name="over50" type="int" />
        <Attribute issuanceMode="known" name="over60" type="int" />
        <Attribute issuanceMode="known" name="over65" type="int" />
        <Attribute issuanceMode="known" name="over75" type="int" />
    </Attributes>

    <Features/>

    <Implementation>
        <AttributeOrder>
            <Attribute name="expiry">1</Attribute>
            <Attribute name="over50">2</Attribute>
            <Attribute name="over60">3</Attribute>
            <Attribute name="over65">4</Attribute>
            <Attribute name="over75">5</Attribute>
        </AttributeOrder>
    </Implementation>

</CredentialStructure>
