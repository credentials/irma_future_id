<?xml version="1.0" encoding="UTF-8"?>
<CredentialStructure xmlns="http://www.zurich.ibm.com/security/idemix"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" 
    xmlns:xs="http://www.w3.org/2001/XMLSchema"
    xsi:schemaLocation="http://www.zurich.ibm.com/security/idemix CredentialStructure.xsd">
    
    <Attributes>
        <Attribute issuanceMode="known" name="expiry" type="int" />
        <Attribute issuanceMode="known" name="dateofbirth" type="int" />
        <Attribute issuanceMode="known" name="placeofbirth" type="int" />
        <Attribute issuanceMode="known" name="countryofbirth" type="int" />
        <Attribute issuanceMode="known" name="gender" type="int" />
    </Attributes>

    <Features/>

    <Implementation>
        <AttributeOrder>
            <Attribute name="expiry">1</Attribute>
            <Attribute name="dateofbirth">2</Attribute>
            <Attribute name="placeofbirth">3</Attribute>
            <Attribute name="countryofbirth">4</Attribute>
            <Attribute name="gender">5</Attribute>
        </AttributeOrder>
    </Implementation>

</CredentialStructure>
